{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "type": {
            "type": "string"
          },
          "attributes": {
            "type": "object",
            "properties": {
              "keyword_id": {
                "type": "number"
              },
              "urls": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            },
            "required": [
              "keyword_id",
              "urls"
            ]
          }
        }
      },
      "required": ["id", "type", "attributes"]
    }
  },
  "required": ["data"]
}
