<tr data-keyword-id="<%= presenter.keyword.id %>" id="<%= "keyword_#{presenter.keyword.id}" %>">
  <td><%= presenter.keyword.id %></td>
  <td><%= presenter.keyword.content %></td>
  <td data-testid="status">
    <span class="<%= presenter.status_badge %>">
      <%= presenter.keyword.status.capitalize %>
    </span>
  </td>
  <td><%= presenter.keyword.created_at.strftime("%H:%M, %-d %B") %></td>
  <td><%= link_to(I18n.t("keywords.view_detail_link"), keyword_path(presenter.keyword)) %></td>
</tr>
